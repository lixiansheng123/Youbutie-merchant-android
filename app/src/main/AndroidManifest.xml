    <application
        android:name=".app.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OrderDetailActivity" />
        <activity android:name=".ServiceEvaluateActivity" />
        <activity android:name=".MerchantCollectionActivity" />
        <activity android:name=".SendMessageActivity" />
        <activity android:name=".SelectCarActivity" />
        <activity android:name=".InfoEditActivity" />
        <activity android:name=".AdListActivity" />
        <activity android:name=".UserListActivity" />
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask" />
        <activity android:name=".ClientDetailActivity" />
        <activity android:name=".InviteMemberActivity" />
        <activity android:name=".SearchAddressBookFriendActivity" />
        <activity android:name=".ServiceListActivity" />
        <activity android:name=".EditMerchantActivity" />
        <activity android:name=".CustomMerchantServiceActivity" />
        <activity
            android:name=".IncomeDetailActivity"
            android:launchMode="singleTask" />
        <activity android:name=".ContributionRankingActivity" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".ApplyWithdrawActivity"
            android:label="@string/title_activity_apply_withdraw"
            android:theme="@style/AppTheme" />
        <activity android:name=".ApplySucceedActivity" />
        <activity android:name=".WithdrawRecordActivity" />
        <activity android:name=".MonthOrderDetailActivity" />
        <activity android:name=".ForgetPsdActivity" />
        <activity android:name=".MerchantSettingActivity" />
        <activity android:name="com.umeng.fb.ConversationActivity" />

        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="${UMENG_CHANNEL_VALUE}" />
        <meta-data
            android:name="UMENG_APPKEY"
            android:value="56936d5867e58e10fe001b1e" />

        <!-- umeng应用更新 -->
        <service
            android:name="com.umeng.update.net.DownloadingService"
            android:process=":DownloadingService" />

        <activity
            android:name="com.umeng.update.UpdateDialogActivity"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".GiftDetailActivity" />
        <activity android:name=".GetSucceedActivity"></activity>
    </application>

