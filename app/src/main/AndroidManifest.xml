<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yuedong.youbutie_merchant_android">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".app.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OrderDetailActivity" />
        <activity android:name=".ServiceEvaluateActivity" />
        <activity android:name=".MerchantCollectionActivity" />
        <activity android:name=".SendMessageActivity" />
        <activity android:name=".SelectCarActivity" />
        <activity android:name=".InfoEditActivity" />
        <activity android:name=".AdListActivity" />
        <activity android:name=".UserListActivity" />
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask" />
        <activity android:name=".ClientDetailActivity" />
        <activity android:name=".InviteMemberActivity" />
        <activity android:name=".SearchAddressBookFriendActivity" />
        <activity android:name=".ServiceListActivity" />
        <activity android:name=".EditMerchantActivity" />
        <activity android:name=".CustomMerchantServiceActivity" />
        <activity android:name=".IncomeDetailActivity" />
        <activity android:name=".ContributionRankingActivity"></activity>
    </application>

</manifest>
